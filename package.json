  },
  "homepage": "https://github.com/eeroan/tennishelsinki",
  "dependencies": {
    "baconjs": "^0.7.83",
    "chartist": "^0.9.5",
    "cheerio": "^0.20.0",
    "dateutils": "^0.2.1",
    "express": "^4.13.3",
    "express-babelify-middleware": "^0.2.1",
    "fastclick": "^1.0.6",
    "forever": "^0.15.1",
    "jquery": "^2.1.4",
    "lodash": "^4.6.1",
    "mongodb": "^2.1.2",
    "newrelic": "^1.24.0",
