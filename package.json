{
  "name": "tennishelsinki",
  "version": "1.0.0",
  "description": "Lists tennis courts of helsinki in one view",
  "main": "server.js",
  "scripts": {
    "test": "NEW_RELIC_ENABLED=false mocha",
    "start": "NEW_RELIC_ENABLED=true forever ./server.js",
    "dev": "supervisor ./server.js",
